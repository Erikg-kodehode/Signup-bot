# Discord Bot Configuration (Required)
DISCORD_BOT_TOKEN=your-bot-token-here
DISCORD_CHANNEL_ID=your-channel-id
DISCORD_GUILD_ID=your-guild-id
DISCORD_ADMIN_ROLE_ID=your-admin-role-id
DISCORD_GUILD_NAME=Your Server Name

# Sign-in Settings (Optional - defaults shown)
SIGN_IN_HOUR=9
SIGN_IN_MINUTE=0

# Logging Configuration (Optional - defaults shown)
LOG_LEVEL=Information
MAX_LOG_SIZE=10m
MAX_LOG_FILES=3

# Development Settings (only used with docker-compose.dev.yml)
DEV_PORT=5000

